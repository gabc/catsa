# Page inexistante
ErrorDocument 404 http://localhost/catsa-web/Web/error.php?code=404

# erreur interne
ErrorDocument 500 http://localhost/catsa-web/Web/error.php?code=500

# Peut ne pas être nécessaire.
Options +FollowSymlinks
RewriteEngine on

# Si le nom du fichier + ".php" est un fichier (-f)
RewriteCond  %{REQUEST_FILENAME}.php -f

# Alors, modifié le fichier par fichier.php.  [L] = break... donc fin de htaccess
RewriteRule ^(.*)$ $1.php

#RewriteRule ^([^/\\.]+)*[/]*([^/\\.]+)*[/]*([^/\\.]+)*[/]*$ index.php?dir1=$1&dir2=$2 [L]
RewriteRule ^tableaux.php[/]*([^/\\.]+)*$ tableaux.php?categories=$1 [L]